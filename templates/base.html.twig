<html>
{% block header %}
    <div id="header"></div>
{% endblock %}

<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
</head>
<body>
{% block body %}

    <div id="body"></div>
{% endblock %}

{% block footer %}

    <div id="footer"></div>

{% endblock %}
</body>

{% block script %}

    <script type="text/javascript" src="{{ asset('build/js/header.js') }}"></script>
    <script type="text/javascript" src="{{ asset('build/js/footer.js') }}"></script>
    <link rel="stylesheet" href="{{ asset('build/js/header.css') }}">
    <link rel="stylesheet" href="{{ asset('build/js/footer.css') }}">
    <script type="text/javascript" src="{{ asset('build/js/app.js') }}"></script>

{% endblock %}
</html>